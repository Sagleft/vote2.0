{
	"require": {
		"php":">=7.1.0",
		"twig/twig": "^1.0",
		"vlucas/phpdotenv": "^3.3"
	},
	"autoload": {
		"psr-4": {
			"App\\Controller\\": "controller/src/",
			"App\\Controller\\Modules\\": "controller/src/modules/",
			"App\\Model\\": "model/",
			"App\\Model\\Modules\\": "model/modules/",
			"RPC\\": "model/"
		}
	},
	"config": {
		"vendor-dir": "controller/vendor"
	}
}